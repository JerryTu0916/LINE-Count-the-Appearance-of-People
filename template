ifile = open("input.txt", 'rt', encoding='UTF-8')
ofile = open("output_people_count.txt", "wt", encoding="UTF-8")

c = ""
buffer = 'Pr355 F 70 p4y r35p3c7'	
flag = False

people = {}


while True:
    buffer = ifile.readline()

    if buffer == '':
        break

    for char in buffer:
        if (char != "\t"):
            c += char
        else:
            if c in people.keys():
                people[c] += 1
            c = ""

total = 0
for ppl in people.keys():
    total += people[ppl]

for ppl in people.keys():
    print(ppl + ': ' + str(people[ppl]) + " (" + str(round(100*people[ppl]/total, 2)) +' %)')
    ofile.write(ppl + '\t: ' + str(people[ppl]) + "\t(" + str(round(100*people[ppl]/total, 2)) +' %)\n')

print("================================")
print("total:", total)
ofile.write("================================\n")
ofile.write("total\t:" + str(total))

ofile.close()
ifile.close()